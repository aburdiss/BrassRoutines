{
  "name": "brass-routines",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "licenses": "npm-license-crawler --onlyDirectDependencies --json src/SettingsStack/Licenses/licenses.json",
    "reset": "./scripts/reset.sh",
    "gen": "./scripts/generateComponent.sh"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.17",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-native-picker/picker": "^2.2.1",
    "@react-native-segmented-control/segmented-control": "^2.4.0",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.1.1",
    "i18n-js": "^3.8.0",
    "lodash.memoize": "^4.1.2",
    "react": "17.0.2",
    "react-native": "0.67.2",
    "react-native-device-info": "^7.2.1",
    "react-native-draggable-flatlist": "^2.5.1",
    "react-native-dynamic": "^1.0.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-haptic-feedback": "^1.11.0",
    "react-native-idle-timer": "^2.1.6",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-localize": "^2.0.1",
    "react-native-modal": "^11.6.1",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^3.10.2",
    "react-native-vector-icons": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@testing-library/react-native": "^7.2.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "node-fetch": "^2.6.1",
    "npm-license-crawler": "^0.2.1",
    "react-test-renderer": "17.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@react-native-community",
    "rules": {
      "eqeqeq": 0
    }
  },
  "babel": {
    "presets": [
      "module:metro-react-native-babel-preset"
    ]
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-image-pan-zoom|react-native-haptic-feedback|react-native-image-zoom-viewer|react-native-animatable|react-native-modal|react-native-dynamic|react-native-iphone-x-helper|react-native-reanimated|react-native-vector-icons|react-native-screens|react-native-splash-screen|react-navigation-tabs|@?react-navigation|react-native-gesture-handler|@react-native-segmented-control/segmented-control|react-native-picker-select|@react-native-picker/picker)/)"
    ]
  }
}
